    agent any
    stages {
        stage('Hello'){
            
            steps {
                echo 'Hello Wrld'
            }
        }
        
        stage('Build'){
            
            steps {
                echo 'Dev'
            }
        }
        stage('Test'){
            
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy'){
            
            steps {
                echo 'Mise en Production'
            }
        }
        
        
    }
}
